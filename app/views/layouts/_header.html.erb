<header>
      <nav>
      <h1><%= link_to image_tag("logo.png"), root_path%></h1>
      <ul class= "left">
        <li>
          <%= link_to "All Events", events_path%>
        </li>
        <li><%= link_to "All Users", users_path%></li>
      </ul>
      <ul class="right">
        <% if current_user%>
        <li><%= link_to current_user.name, current_user %></li>
          <% else %>
        <li>
          <%= link_to "Sign In", new_session_path, class: "button"%>
        </li>
        <li>
          <%= link_to "Sign Up", signup_path, class: "button"%>
        </li>
        <% end %>
      </ul>
      </nav>
    </header>