<table>
  <thead>
    <tr>
      <th>name</th>
      <th>location</th>
      <th>price</th>
    </tr>
  </thead>
  <tbody>
      <% @events.each do |event|%>
      <tr>
        <td><%= event.name%></td>
        <td><%= event.location%></td>
        <td><%= event.price%></td>

      </tr>

      <% end %>
  </tbody>
</table>






<ul>
  <% @events.each do |event|%>
    <li><%= event.name%></li>
  <% end %>
</ul>