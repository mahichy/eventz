<table>
  <thead>
    <tr>
      <th>name</th>
      <th>location</th>
      <th>price</th>
      <th>Description</th>
      <th>Starts at</th>
    </tr>
  </thead>
  <tbody>
      <% @events.each do |event|%>
      <tr>
        <td><%= event.name%></td>
        <td><%= event.location%></td>
        <td><%= number_to_currency(event.price)%></td>
        <td><%= event.description%></td>
        <td><%= event.starts_at%></td>
      </tr>

      <% end %>
  </tbody>
</table>





